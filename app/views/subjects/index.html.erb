<p id="notice"><%= notice %></p>

<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Patient</th>
      <th>Subject name</th>
      <th>Age</th>
      <th>Genter</th>
      <th>Seizure number</th>
      <th>Channels</th>
      <th>Fs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.patient_id %></td>
        <td><%= subject.subject_name %></td>
        <td><%= subject.age %></td>
        <td><%= subject.genter %></td>
        <td><%= subject.seizure_number %></td>
        <td><%= subject.channels %></td>
        <td><%= subject.fs %></td>
        <td><%= link_to 'Show', subject %></td>
        <td><%= link_to 'Edit', edit_subject_path(subject) %></td>
        <td><%= link_to 'Destroy', subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject', new_subject_path %>
